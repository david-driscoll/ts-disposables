{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "es2015.collection"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitOnError": false,
        "declaration": false,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules",
        "rules",
        "jspm_packages",
        "typings/main.d.ts",
        "typings/main"
    ]
}
